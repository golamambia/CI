/** Top Bar End **/


/** Header Start **/
.headerInner .container {
    position: relative;
}
.header-logo {
    position: absolute;
    left: 15px;
    top: -47px;
    z-index: 99;
}
.header-logo img{
    width: 181px;
    height: 190px;
}
.form-section{
    background: #f2f2f2;
}

.navbar{
    border-radius: 0;
    margin-bottom: 0;
    border: none;
    background: linear-gradient(to bottom, #ed8831 0%, #ae5108 100%);
}
.navbar-inverse .navbar-nav>.active>a, 
.navbar-inverse .navbar-nav>.active>a:focus, 
.navbar-inverse .navbar-nav>.active>a:hover{
    background-color:transparent;
    color: #ed8831;
}

.nav>li>a:focus, .nav>li>a:hover {
    background-color: #964605;   
}
.nav>li>a{
    /*color: #FFFFFF;
    font-size: 14px;
    font-weight: bold;*/
	/*width: 120px;*/
	width: 156px;
	padding-left:5px;
	padding-right:5px;
    color: #FFFFFF;
    font-size: 12px;
    font-weight: bold;
	height:70px;
	text-align:center;
	display:table-cell;
	vertical-align:middle;
}

.nav>li>a{ position:relative; overflow:hidden;}
.nav>li:last-child>a{ position:relative; overflow:hidden;width:50px; padding-top:5px; display:flex;}
.nav>li:last-child>a>i{ margin:auto}
.nav>li>a:after{ height:100%; content:" "; width:1px; position:absolute; right:0; background:#000000; top:0;}

/* Slider Section*/
.hero-slider .slider-images{
    background-repeat: no-repeat;
    width: 100%;
    height: 510px;
    position: relative;
    background-size: cover;
    background-position: center;
}
.hero-slider .slider-images::before{
    position: absolute;
    content: "";
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.75);
    top: 0;
    z-index: 1;
}
.slider-images .inner-container{
    position: relative;
    z-index: 2;
}
.slider-copy{
    max-width: 750px;
    width: 100%;
    padding-top:149px;
    margin: 0 auto;
    color: #FFFFFF;
}
.inner-container .slider-copy h2{
    font-size:47px;
    font-weight: 900;
    margin: 0px 0px 20px;
    padding: 0px;
    text-transform: uppercase;
}
.inner-container .slider-copy p{
    font-size: 16px;
    line-height: 1.4;
    font-weight: 400;
    margin: 0px 0px 30px;
    padding: 0;
}
.inner-container .slider-copy .button a{
    margin: 0;
    padding: 13px 39px;
    color: #FFFFFF;
    font-size: 16px;
    font-weight: 600;
    border-radius:0px;
     display: inline-block;
    text-decoration: none;
    text-transform: uppercase;
    background: linear-gradient(to bottom, #ed8831 0%, #ae5108 100%);
}
.inner-container .slider-copy .button a:hover{
    background: #964605;
}
.slick-dots{
    bottom: 12px !important;
}
.slick-dots li{
     margin: 0 2px !important;
}
.slick-dots li button{
    width: 13px!important;
    height: 13px!important;
    background: #cf6d1d !important;
    border-radius: 100%!important;
    border: solid 2px #FFF!important;
}
.slick-dots li.slick-active button{
    background: #FFFFFF!important;
}
.slick-dotted.slick-slider{
    margin-bottom: 0!important;
}

/* Search sec */
.searchSec {
    background: #dedede;
    padding: 30px 0 15px;
}
.searchSec .form-control {
    display: block;
    width: 100%;
    border: none;
    border-radius: 0;
    height: 40px;
}
.searchSec .search-btn {
    display: block;
    width: 100%;
    height: 40px;
    border: none;
    border-radius: 0;
    background: #d37120;
    color: #FFF;
    text-transform: uppercase;
}
/* Search sec */



/* Offer Section*/
.offerSection{
    background: #FFFFFF;
}
.offerSection a{
    text-decoration: none;
}
.offer-slide-box{
    border-right: solid 2px #eee;
    margin: 0 auto;
    max-width: 100%;
    padding: 17px 45px;
}
.border-none{
    border: none;
}
.offer .col-md-3{
    padding-left: 0;
    padding-right: 0;
}
.custom-table{
   display: table;
}
.custom-table .table-cell{
    display: table-cell;
    vertical-align: middle;
}
.offer-slide-icon{
    display: block;
    width: 62px;
    height: auto;
    margin: 0;
    max-width: inherit;
}
.offer-slide-title{
    margin: 0 0 1px;
    padding: 0 0 0 15px;
    color: #303135;
    font-size: 16px;
    font-weight: 900;
}
p.offer-copy{
    margin: 0;
    padding: 0 0 0 15px;
    font-size: 13px;
    color: #555555;
    font-weight: 400;
}
.offerArrow {
    display: none !important;
}

/* Banner Section*/
.no-margin{
    margin-right:0;
    margin-left:0;
}
section#content{
    padding: 70px 0 75px;
}
.intro-copy p{
    margin: 0 0 10px;
    padding: 0;
    font-size: 26px;
    font-weight: 400;
    color: #3b3d3b;
}
.intro-copy h3.sec-title{
    margin: 0 0 25px;
    padding: 0;
    font-size: 26px;
    font-weight: 900;
    color: #3b3d3b;
}
a.cat-large-view{
    text-decoration: none;
    color: #FFFFFF;
}
.cat-large-view{
    display: block;
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center top;
    position: relative;
    height: 100%;
    width: 100%;
}
.cat-large-view:before{
    content: "";
    display: block;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.60);
    position: absolute;
    top: 0;
    left: 0;
}
.cat-info-copy{
    position: relative;
    padding: 20px;
    max-width: 415px;
    width: 100%;
	height: 100%;
/*    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);*/
}


.suparbox{
	position: relative;
    padding: 20px;
    max-width: 415px;
    width: 100%;
   top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
}

.cat-info-copy a,.suparbox a {
    display: inline-block;
    color: #f26522;
    font-size: 14px;
    font-weight: 600;
    letter-spacing: 1px;
    background: #FFFFFF;
    padding: 15px 43px;
    margin: 0;
    border-width: 1px;
    border-style: solid;
    border-color: rgb(255, 255, 255);
    border-image: initial;
    /*border-radius: 27px;*/
    outline: 0px;
    text-decoration: none;
}
.cat-info-copy a:hover,
.suparbox a:hover {
    color: #FFF;
    background: #f26522;
}
.products-right.cat-info-copy a,
.products-right.suparbox a {
    padding: 7px 24px;
}
.last-banner .cat-info-copy a,
.last-banner .suparbox a {
    float: right;
}
.cat-info-copy h4,
.suparbox h4{
    margin: 0 0 18px;
    padding: 0;
    color: #FFF;
    font-size:38px;
    line-height: 1.3;
    font-weight: 900;
    text-transform: uppercase;
    text-shadow: rgb(0, 0, 0) 2px 2px 10px;
}
.cat-info-copy p,
.suparbox p{
    margin: 0 0 26px;
    padding: 0;
    font-size: 16px;
    color: #FFF;
    font-weight: 400;
    text-shadow: rgb(0, 0, 0) 2px 2px 10px;
}
.pro-cat-box{
    width: 50%;
    height: 260px;
    min-height: 50%;
    position: relative;
}
.pro-thumb{
    position: relative;
}
.cat-small-view{
    position: relative;
}
.pro-thumb img{
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
    position: relative;
    height: 100%;
    width: 100%;
}
.pro-thumb .product-copy{
    position: absolute;
    top: 50%;
}
.last-banner .cat-large-view {
    margin-top: 30px;
}

.last-banner .cat-large-view .cat-info-copy {
    max-width: 100%;
        top: inherit;
    transform: translateY(0);
    -webkit-transform: translateY(0);
}

.products-right h4{
    font-size: 20px;
    margin: 0 0 8px;
}
.last-banner .products-right h4 {
    margin: 0;
    padding: 7px 0;
    float: left;
}
.products-right p{
    font-size: 16px;
    color: #FFF;
}
.pro-cat-box-in{
    position: relative;
    width: 100%;
    height: 100%;
}
.product-banner-img img{
        vertical-align: top;
    max-width: 100%;
    height: auto;
    position: relative;
}
.pro-banner-copy{
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    padding: 15px;
}

