img {
    vertical-align: top;
    max-width: 100%;
    height: auto;
}
/** Top Bar Start **/
.topBar-menu{
    background: #000000;
}
.topBar-copy{
   padding: 4px 0; 
}

.time-copy {
    display: inline-block;
    vertical-align: middle;
}
.topBar-social-menu {
    display: inline-block;
    vertical-align: middle;
}


.topBar-menu p{
    margin: 0 15px 0 0;
    padding: 3px 0 0 0;
    color: #FFFFFF;
    font-size: 13px;
    font-weight: 400;
    text-transform: uppercase;
}
.topBar-social-menu ul{
    margin: 0;
    padding: 0;
    list-style: none;
}
.topBar-social-menu > ul > li{
    display: inline-block;
    margin: 0 0 0 20px;
    position: relative;
}
.topBar-social-menu > ul > li > a{
    margin: 0;
    padding: 0;
    font-size: 14px;
    color: #FFFFFF;
    text-align: center;
    vertical-align: middle;
    text-decoration: none;
}

@media(min-width:900px){
.fix-box{ height: 250px!important; width: 250px!important; margin:0  auto;}
}

.topBar-social-menu li a:hover{
    color: #ed8831;
}
.topBar-social-menu > ul > li > a > span{
    padding: 2px 0 0 0;
    margin-right: 10px;
    background: #2f2f2f;
    width: 25px;
    height: 25px;
    border-radius: 100%;
    display: inline-block;
}


