ul.rating {
    margin: 0;
    padding: 0;
    text-align: center;
    color: #f3b800;
}
.rating li.fa-star{
    display: inline-block;
}
.fa-star{
  content: "\f006";
    font-family: FontAwesome;
    font-size: 16px;
    z-index: 99;
}
.fa-star:before{content: "\f006"!important;}
.slArrow{
    background: #333333;
    width: 34px;
    height: 34px;
    text-align: center;
    line-height: 34px;
    color: #FFF;
    font-size: 14px;
    cursor: pointer;
    position: absolute;
    top: -60px;
    z-index: 9;
}
.slArrow.slick-arrow-left{
    right: 44px;
}
.slArrow.slick-arrow-right{
        right: 0;
}
/* Products category */
.products-list .equalheight {
    margin-bottom: 15px;
    margin-top: 15px;
}
.category-productsBox{
    padding: 35px 27px;
    background: #FFFFFF;
       border: solid 2px #ccc;
    position: relative;
	height: 100%;
	padding-bottom: 30px;
}

@media(max-width:800px){
	.category-productsBox{padding-bottom: 70px;}
	
}

.category-productsBox .category-img {
    float: left;
    max-width: 35%;
	height: 140px;
	width: 140px;
}
.category-productsBox .category-img img{
	height: 140px;
	width: 140px;
	    border: 2px solid #ccc;
    padding: 5px;
	-webkit-transition: all 200ms ease;
-moz-transition: all 200ms ease;
-ms-transition: all 200ms ease;
-o-transition: all 200ms ease;
transition: all 200ms ease;
}

.category-productsBox .category-img img:hover{
	height: 140px;
	width: 140px;
	transform: scale(1.2);
}

.category-productsBox .category-product-list {
    float: right;
    width: 60%;
}

.category-product-list h5{
    margin: 0 0 2px;
    padding: 0;
    color: #d37120;
    font-size: 16px;
    font-weight: 900;
}
.category-product-list ul{
    margin: 0;
    padding: 0;
    list-style: none;
}
.category-product-list ul li{
    margin: 0 0 3px;
}
.category-product-list ul li a{
    font-size: 13px;
    color: #303135;
    font-weight: 400;
    text-decoration: none;
}
.category-product-list ul li a:hover{
    color: #ed8831;
}
.category-productsBox .btn {
    background: linear-gradient(to bottom, #ed8831 0%, #ae5108 100%);
    border: 0;
    color: #fff;
    font-size: 16px;
    font-weight: 300;
}




